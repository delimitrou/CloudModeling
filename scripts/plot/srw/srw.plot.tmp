set size 1,1
set terminal postscript enhanced color dashed lw 1 "Helvetica" 14
set output "srw.eps"
set title "Sequential Read/Write Throughput"
set xlabel "Working Set Size (KB)"
set ylabel "Throughput (GB/s)"
set yrange [0:30]
set xrange [8:1048576]
set logscale x 10
set key horizontal
set style line 1 lt 2 lc rgb "#999999" lw 1 dashtype 2
set arrow from 32,0 to 32,30 nohead ls 1
set label 1 "L1 Data Cache 32KB" at 32,27 tc rgb "#999999"
set arrow from 6144,0 to 6144,30 nohead ls 1
set label 2 "L2 Smart Cache" at 6144,27 tc rgb "#999999"

plot \
 'srw_raw.dat' u 1:2 with points title "read" ps .3 lc rgb "#FF0000", \
 'srw_raw.dat' u 1:3 with points title "write" ps .3 lc rgb "#0000FF", \
 'srw_avg.dat' u 1:2 with lines title "read-avg" lw 2 lc rgb '#880000', \
 'srw_avg.dat' u 1:3 with lines title "write-avg" lw 2 lc rgb '#000088'
